{
  "name": "sxastarter",
  "description": "Application utilizing Content SDK and Next.js",
  "version": "1.1.0",
  "private": true,
  "config": {
    "appName": "sxastarter",
    "rootPlaceholders": [
      "jss-main"
    ],
    "sitecoreConfigPath": "/App_Config/Include/zzz",
    "graphQLEndpointPath": "/sitecore/api/graph/edge",
    "language": "en",
    "templates": [
      "nextjs",
      "nextjs-xmcloud",
      "nextjs-sxa",
      "nextjs-multisite"
    ]
  },
  "author": {
    "name": "Sitecore Corporation",
    "url": "https://jss.sitecore.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sitecore/jss.git"
  },
  "bugs": {
    "url": "https://github.com/sitecore/jss/issues"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@sitecore-cloudsdk/core": "^0.5.1",
    "@sitecore-cloudsdk/events": "^0.5.1",
    "@sitecore-cloudsdk/search": "^0.5.4",
    "@sitecore-content-sdk/nextjs": "^1.2.1",
    "@sitecore-feaas/clientside": "^0.6.0",
    "@sitecore-search/react": "^3.0.0",
    "@sitecore-search/ui": "^3.0.0",
    "@sitecore/components": "~2.1.0",
    "bootstrap": "^5.1.3",
    "font-awesome": "^4.7.0",
    "next": "15.3.8",
    "next-localization": "^0.12.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-scroll-parallax": "^3.4.5",
    "react-share": "^5.1.0",
    "sass": "^1.52.3",
    "sass-alias": "^1.0.5",
    "sharp": "0.32.6",
    "swiper": "^11.1.4"
  },
  "devDependencies": {
    "@sitecore-content-sdk/cli": "~1.2.1",
    "@sitecore-search/cli": "^2.6.3",
    "@storybook/addon-essentials": "^8.0.8",
    "@storybook/nextjs": "^8.0.8",
    "@storybook/react": "^8.0.8",
    "@types/node": "^22.9.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "chalk": "~4.1.2",
    "chokidar": "~3.5.3",
    "constant-case": "^3.0.4",
    "cross-env": "~7.0.3",
    "dotenv-flow": "^4.1.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^13.1.5",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-yaml": "^0.5.0",
    "npm-run-all": "~4.1.5",
    "prettier": "^2.8.3",
    "storybook": "^8.0.8",
    "stylelint": "^16.3.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-scss": "^6.2.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "~5.4.0",
    "yaml-loader": "^0.8.0",
    "@sitecore-content-sdk/nextjs": "^1.2.1"
  },
  "scripts": {
    "DEMO_TEAM_CUSTOMIZATION_EXTRA_SCRIPTS": "",
    "storybook": "npm-run-all --serial bootstrap storybook:start",
    "storybook:start": "storybook dev -p 6006",
    "build-storybook": "npm-run-all --serial bootstrap storybook:build",
    "storybook:build": "storybook build",
    "build": "cross-env NODE_ENV=production npm-run-all --serial sitecore-tools:generate-map sitecore-tools:build next:build",
    "eslint": "eslint ./src/**/*.tsx ./src/**/*.ts ./scripts/**/*.ts",
    "stylelint": "stylelint ./src/**/*.scss",
    "stylelint-fix": "stylelint ./src/**/*.scss --fix",
    "lint": "eslint ./src/**/*.tsx ./src/**/*.ts",
    "next:build": "next build",
    "next:dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "next:start": "next start",
    "sitecore-tools:generate-map": "sitecore-tools project component generate-map",
    "sitecore-tools:generate-map:watch": "sitecore-tools project component generate-map --watch",
    "sitecore-tools:build": "sitecore-tools project build",
    "dev": "cross-env NODE_ENV=development npm-run-all --serial sitecore-tools:build --parallel next:dev sitecore-tools:generate-map:watch",
    "start": "cross-env-shell NODE_ENV=production npm-run-all --serial build next:start"
  }
}
